created:
    - database/factories/EmployeeFactory.php
    - database/factories/DepartmentFactory.php
    - database/factories/ProjectFactory.php
    - database/migrations/2023_02_07_170305_create_employees_table.php
    - database/migrations/2023_02_07_170306_create_departments_table.php
    - database/migrations/2023_02_07_170307_create_projects_table.php
    - database/migrations/2023_02_07_170308_create_employee_project_table.php
    - app/Models/Employee.php
    - app/Models/Department.php
