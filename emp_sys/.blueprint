models:
    User: { name: string, email: string, email_verified_at: 'datetime nullable', password: string, remember_token: 'string:100 nullable' }
    Employee: { name: 'string:100', identification: 'string:100', birth: datetime, salary: unsignedinteger, martial_status: 'enum:single,married,divorced', bonus: 'decimal:8,2 nullable', order: 'unsignedinteger default:1 index', department_id: 'id foreign:department.id', relationships: { belongsToMany: Project } }
    Department: { name: 'string:100', relationships: { hasMany: Employee } }
    Project: { name: 'string:100', relationships: { belongsToMany: Employee } }
